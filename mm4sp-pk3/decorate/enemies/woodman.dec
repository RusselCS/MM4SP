actor Robbit : GenericRobot 30047
{
//$Category MM8BDM-Enemies
Health 50
Height 64
Radius 64
Obituary "%o was gnawed on by a Robbit."
maxstepheight 50
+DROPOFF
+LOOKALLAROUND
States
{
Spawn:
TNT1 A 0
TNT1 A 0 ACS_ExecuteAlways(400,0)
Look:
ROBB A 0 A_ChangeFlag("SOLID",1)
ROBB A 6 A_LookEx(LOF_NOSOUNDCHECK,0,2000,0,360,"See")
loop
See:
ROBB A 20 A_FaceTarget
ROBB A 12 A_FaceTarget 
ROBB A 0 A_Recoil(-15)
ROBB A 1 A_FaceTarget
ROBB A 10 ThrustThingZ(0,50,0,0)
Goto Leaping
Leaping:
ROBB B 0 A_SpawnItemEx("RobbitStomp")
ROBB B 1 A_FaceTarget
ROBB B 1 A_JumpIf(Floorz-z==0,"Land")
loop
Land:
ROBB A 1 A_FaceTarget
ROBB A 4 A_Stop
ROBB AAAAA 6 A_FaceTarget
ROBB A 0 A_FaceTarget
ROBB A 20 A_CustomMissile("RobbitCarrot",32,8,random(-4,4))
ROBB A 0 A_FaceTarget
ROBB A 20 A_CustomMissile("RobbitCarrot",32,8,random(-4,4))
ROBB A 0 A_FaceTarget
ROBB A 20 A_CustomMissile("RobbitCarrot",32,8,random(-4,4))
ROBB A 2 A_ClearTarget
Goto Look
}
}

actor RobbitStomp
{
-SOLID
PROJECTILE
damagetype "Enemy"
States
{
Spawn:
TNT1 A 0
TNT1 A 1 A_Explode(20,76,0)
stop
}
}

actor RobbitCarrot
{
PROJECTILE
Damage (15)
Obituary "%o was pelted by a Robbit."
damagetype "Enemy"
Speed 20
scale 2.5
Height 16
Radius 16
States
{
Spawn:
RCAR A 1
loop
}
}

