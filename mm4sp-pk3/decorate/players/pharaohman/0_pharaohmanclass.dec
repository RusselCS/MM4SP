actor PharaohManSP : ClassBaseSP
{
player.displayname "pharaohman"

player.startitem "BusterAmmo", 1
player.startitem "PharaohManWep"
player.startitem "PharaohManWep2"
player.startitem "PharaohManAmmo2", 28
player.startitem "PharaohManFlag"
player.startitem "EnergyBalancer"
player.jumpz 12
speed 1.15

States
{
Spawn:
PHAR A 0
PHAR B 1 A_GiveInventory("FrameBGive",1)
PHAR A 1 A_GiveInventory("FrameAGive",1)
Goto Spawn+2
See:
PHAR B 5 A_GiveInventory("FrameBGive",1)
PHAR C 5 A_GiveInventory("FrameCGive",1)
PHAR D 5 A_GiveInventory("FrameDGive",1)
PHAR E 5 A_GiveInventory("FrameEGive",1)
Goto Spawn
Missile:
PHAR FFFFF 1 A_GiveInventory("FrameFGive",1)
PHAR GGGG 1 A_GiveInventory("FrameGGive",1)
goto Spawn
ClassPain:
PHAR H 0 A_GiveInventory("FrameHGive",1)
goto PainContinue
DeathFix:
PHAR H 0
goto DeathContinue
ClassDeath:
PHAR H 1
goto MegaDeathEnd
}
}

actor Frame : Inventory
{
inventory.amount 1
inventory.maxamount 1
}

actor FrameA : Frame {}
actor FrameB : Frame {}
actor FrameC : Frame {}
actor FrameD : Frame {}
actor FrameE : Frame {}
actor FrameF : Frame {}
actor FrameG : Frame {}
actor FrameH : Frame {}

actor FramesTake : CustomInventory
{
States
{
Pickup:
TNT1 A 0 A_TakeInventory("FrameA", 1)
