actor SP_Taban : GenericRobot 30032
{
//$Category MM8BDM-Enemies
Height 40
Radius 32
Health 35
scale 2.5
species "Telly"
Obituary "%o was killed by a Taban."
+SOLID
+NOGRAVITY
+LOOKALLAROUND
+INVULNERABLE
dropitem "WeaponEnergy", 32
dropitem "SmallHealth", 32
speed 4
floatspeed 3
States
{
Spawn:
TNT1 A 0
TNT1 A 0 A_JumpIfInventory("HealthScaled",1,"SpawnLoop")
TNT1 A 0 ACS_NamedExecuteAlways("4SP_EnemyStart",0)
TNT1 A 0 A_GiveInventory("DamageCounter",15)
TNT1 A 0 A_SpawnItemEX("BasicEnemyContactDamager")
SpawnLoop:
TABA C 0 A_ClearTarget
TABA C 0 A_ChangeFlag("FLOAT",0)
TABA C 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA C 1 ThrustThingZ(0,65,0,0)
TABA C 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA C 1 ThrustThingZ(0,65,0,0)
TABA C 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA C 1 ThrustThingZ(0,65,0,0)
TABA C 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA C 1 ThrustThingZ(0,65,0,0)
TABA C 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA C 1 ThrustThingZ(0,65,0,0)
TABA D 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA D 1 ThrustThingZ(0,65,0,0)
TABA D 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA D 1 ThrustThingZ(0,65,0,0)
TABA D 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA D 1 ThrustThingZ(0,65,0,0)
TABA D 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA D 1 ThrustThingZ(0,65,0,0)
TABA D 0 A_JumpIf(ceilingz-z<=40,"OperationConsistentHeight")
TABA D 1 ThrustThingZ(0,65,0,0)
loop
OperationConsistentHeight:
TABA A 0 A_ChangeFlag("INVULNERABLE",1)
TABA A 1 ThrustThingZ(0,-4*(41-(ceilingz-z)),0,0)
goto Look
Look:
TABA A 0 A_Stop
TABA A 0 ThrustThingZ(0,0,0,0)
TABA A 5 A_LookEx(LOF_NOSOUNDCHECK,0,300,0,360,"See2")
Goto Look+1
See:
TABA CCCCCDDDDDCCCCCDDDDDCCCCCDDDDDCCCCCDDDDDCCCCC 1 A_Chase("")
TABA D 1 A_FaceTarget
TABA D 1 A_CustomMissile("TabanShot",2,0,0,0)
TABA DDD 1 A_Chase
loop
See2:
TABA A 150
APAC B 0 A_PlaySoundEX("enemy/taban","SoundSlot6")
TABA B 5 A_GiveInventory("CutterFlag",1)
TABA B 1 A_FaceTarget
TABA A 0 A_ChangeFlag("FLOAT",1)
TABA A 0 A_ChangeFlag("INVULNERABLE",0)
TABA A 0 A_CustomMissile("TabanShot",2,0,0,0)
Goto See
Pain.PunctureNeedle:
CRAZ A 0 A_Stop
CRAZ A 0 A_Pain
TABA A 30 A_SpawnItemEx("ShockFX",4,0,32)
Goto Look
Death:
TABA A 0 A_Pain
TABA A 0 A_Fall
TABA A 0 A_ChangeFlag("NOGRAVITY",1)
MMFX BCDE 3
stop
}
}

