actor Pipi : GenericRobot 30051
{
//$Category MM8BDM-Enemies
Health 10
Height 32
Radius 16
maxstepheight 50
+DROPOFF
+NOGRAVITY
+FLOATBOB
-COUNTKILL
States
{
Spawn:
TNT1 A 0
TNT1 A 0 ACS_ExecuteAlways(400,0)
Look:
TNT1 A 0 A_ChangeVelocity(12.0, 0.0, 0.0, CVF_RELATIVE|CVF_REPLACE)
PIPI ABABABABAB 2 //A_LookEx(LOF_NOSOUNDCHECK,0,400,0,360,"See")
goto See//loop
Fly:
PIPI CD 2
loop
See:
TNT1 A 0 A_SpawnItemEx("PipiEgg",0,0,-8,0,0,0,0,SXF_SETMASTER)
Goto Fly
Death:
TNT1 A 0 A_Pain
TNT1 A 0 A_KillChildren
TNT1 A 0 A_Stop
TNT1 A 0 A_ChangeFlag("NOGRAVITY",1)
TNT1 A 0 A_Fall
MMFX BCDE 3
stop
}
}

actor PipiEgg : GenericRobot
{
+THRUSPECIES
-SOLID
-COUNTKILL
scale 2.5
Health 1
Height 16
Radius 16
deathsound "misc/devilpain"
States
{
Spawn:
TNT1 A 0
TNT1 A 0 ThrustThingZ(0,5,0,0)
SpawnLoop:
PIPI E 1 A_JumpIf(velz==0,"ExDeath")
loop
ExDeath:
PIPI F 1 A_SpawnItemEx("Copipi",0,0,16)
PIPI F 1 A_SpawnItemEx("Copipi",64,0,32)
PIPI F 1 A_SpawnItemEx("Copipi",-64,0,16)
PIPI F 1 A_SpawnItemEx("Copipi",0,64,32)
PIPI F 1 A_SpawnItemEx("Copipi",0,-64,16)
PIPI F 5
stop
Death:
PIPI F 1 A_JumpIf(floorz-z==0,"ExDeath")
TNT1 A 0 A_Pain
TNT1 A 0 A_Fall
TNT1 A 0 A_Stop
TNT1 A 0 A_ChangeFlag("NOGRAVITY",1)
MMFX BCDE 3
stop
}
}

