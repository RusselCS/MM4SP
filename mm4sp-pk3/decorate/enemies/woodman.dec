actor SP_Monking : GenericRobot 30049
{
//$Category MM8BDM-Enemies
Health 30
painchance 0
Height 64
Radius 48
Obituary "%o was jumped on by a Monking."
+NOGRAVITY
speed 0
maxstepheight 50
painsound "misc/devilpain"
+DROPOFF
+LOOKALLAROUND
States
{
Spawn:
TNT1 A 0
TNT1 A 0 A_JumpIfInventory("HealthScaled",1,"Look")
TNT1 A 0 ACS_NamedExecuteAlways("4SP_EnemyStart",0)
TNT1 A 0 A_GiveInventory("DamageCounter",20)
TNT1 A 0 A_SpawnItemEX("BasicEnemyContactDamager")
SpawnLoop:
MONK D 0 A_JumpIf(ceilingz-z<=100,"OperationConsistentHeight")
MONK D 1 ThrustThingZ(0,65,0,0)
loop
OperationConsistentHeight:
MONK D 1 ThrustThingZ(0,-4*(101-(ceilingz-z)),0,0)
goto WoofWoof
WoofWoof:
MONK A 0 A_Stop
MONK D 0 ThrustThingZ(0,0,0,0)
MONK ABCB 6 A_LookEx(LOF_NOSOUNDCHECK,0,300,0,360,"SeeDrop")
loop
SeeDrop:
MONK D 0 A_FaceTarget
TNT1 A 0 A_ChangeFlag("NOGRAVITY",0)
MONK D 1 ThrustThing(angle*256/360,5,0,0)
Goto Leaping
See:
MONK D 0 A_FaceTarget
TNT1 A 0 A_ChangeFlag("NOGRAVITY",0)
MONK D 0 ThrustThingZ(0,50,0,0)
MONK D 1 ThrustThing(angle*256/360,5,0,0)
TNT1 A 0 A_ChangeFlag("NOGRAVITY",0)
Goto Leaping
Leaping:
MONK D 1 A_FaceTarget
MONK D 1 A_JumpIf(Floorz-z==0,"Land")
loop
Land: 
Wait:
MONK A 0 A_Stop
MONK AAA 0 A_Chase
MONK E 0 A_FaceTarget
MONK E 25
Look:
MONK E 1 A_LookEx(LOF_NOSOUNDCHECK,0,500,0,360,"See")
loop
Death:
CRAZ A 0 A_Pain
CRAZ A 0 A_Fall
CRAZ A 0 A_SpawnItem("EnemyDeathFX",0,32)
TNT1 A 1
stop
}
}

